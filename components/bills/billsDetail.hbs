<!-- Main content -->
<section class="content">

    <!-- Default box -->
    <div class="card">
        <div class="card-body row">
            <div class="col-5 text-center d-flex align-items-center justify-content-center">
                <div class="">
                    <img src="/img/users/avatar08.jpg" alt="" />
                </div>
            </div>
            {{#each bills}}
            <div class="col-7">
                <h5><strong>ID</strong></h5>
                <p class="text-muted">{{this.ID}}</p>
                <h5><strong>Tên Khách Hàng</strong></h5>
                <p class="text-muted">{{this.TenKhachHang}}</p>
                <h5><strong>Địa chỉ</strong></h5>
                <p class="text-muted">{{this.DiaChi}}</p>
                <h5><strong>Tổng Tiền</strong></h5>
                <p class="text-muted">{{this.TongTien}}</p>
                <h5><strong>Danh Sách Phiếu Thuê</strong></h5>
                <a href="/rentals">
                    <p class="text-muted" >{{this.DanhSachPhieuThue}}</p>
                </a>
            </div>
            {{/each}}
        </div>
    </div>

</section>
<!-- /.content -->